 2  apt-get update -y
    3  apt install openjdk-11-jdk
    4  useradd -m -U -d /opt/tomcat -s /bin/false tomcat
    5  wget https://downloads.apache.org/tomcat/tomcat-9/v9.0.72/bin/apache-tomcat-9.0.72.tar.gz
    6  sudo tar -xf apache-tomcat-9.0.72.tar.gz -C /opt/tomcat/
    7  cd /opt/tomcat/
    8  ls
    9  cd apache-tomcat-9.0.72/
   10  ls
   11  cd bin/
   12  ls
   13  cd
   14  sh /opt/tomcat/apache-tomcat-9.0.72/bin/./startup.sh 



